{
    "name": "covid19_tracemap",
    "version": "2021.1.0",
    "description": "Trace Map for COVID-19",
    "main": "webpack.config.js",
    "scripts": {
        "build": "SET NODE_ENV=production& webpack -p --config webpack.config.js",
        "build_dev": "SET NODE_ENV=production& webpack -p --config webpack.config.dev.js",
        "build_es5": "SET NODE_ENV=production& webpack -p --config webpack.config.babel.js",
        "dev": "SET NODE_ENV=development& webpack-dev-server --progress --hot --content-base build --config webpack.config.dev.js",
        "lint": "eslint ./**/*",
        "test": "jest --no-cache --config jest.config.js",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage --colors",
        "release": "git fetch upstream --tags && npm run release:patch && npm run release:push:upstream && npm run build",
        "release:patch": "standard-version --release-as patch",
        "release:minor": "standard-version --release-as minor",
        "release:major": "standard-version --release-as major",
        "release:push:upstream": "git push --follow-tags upstream HEAD:develop"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:michelle800902/COVID-19-TraceMap.git"
    },
